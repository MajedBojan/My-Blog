<%= form_for  @user do |f|%>

<p>
<%=	f.label :username %><br>
<%= f.text_field :username %>
</p>

<p>
<%=	f.label :email %><br>
<%= f.text_field :email %>
</p>
<p>
<%=	f.label :confirmation %><br>
<%= f.text_field :email_confirmation %>
</p>
<p>
<%=	f.label :password %><br>
<%= f.password_field :password %>
</p>

<p>
<%=	f.label :privilege %><br>
<%= f.check_box :privilege %>
</p>

<p>
<%= f.label :image %><br>
<%= f.file_field :image %>
</p>

<%= f.submit%>
<%end%>