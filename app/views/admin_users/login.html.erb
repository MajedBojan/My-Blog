<% if !flash[:notice].blank?%>
<div class="sign_in">
<%= flash[:notice]%>

</div>
<%end%>
<div class="">
<%= form_tag(:controller => 'admin_users',:action => 'attempt_login') do %>
<p>
<%=	label_tag(:username) %><br>
<%= text_field_tag(:username) %>
</p>


<p>
<%= label_tag(:password) %><br>
<%= password_field_tag(:password )%>
</p>

<p>
<%= submit_tag("Log In") %>
</p>
<% end %>
</div>